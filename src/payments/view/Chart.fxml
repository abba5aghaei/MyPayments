<AnchorPane prefHeight="498.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="payments.view.ChartController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="450.0" prefWidth="620.0">
         <children>
            <BarChart fx:id="barChart" layoutX="40.0" layoutY="25.0" prefHeight="450.0" prefWidth="620.0" title="نمودار پرداختهای ماهانه" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
