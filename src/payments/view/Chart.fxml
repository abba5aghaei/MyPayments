                  <CategoryAxis side="BOTTOM" fx:id="xAxis" />
               </xAxis>
               <yAxis>
                  <NumberAxis side="LEFT" />
               </yAxis>
            </BarChart>
         </children>
      </AnchorPane>
      <ComboBox fx:id="list" layoutX="367.0" layoutY="458.0" onAction="#selectYear" prefWidth="150.0" AnchorPane.bottomAnchor="15.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
      </ComboBox>
      <Label layoutX="529.0" layoutY="458.0" text="انتخاب سال:" AnchorPane.bottomAnchor="15.0">
         <font>
            <Font size="16.0" />
         </font>
         <effect>
            <DropShadow />
         </effect>
      </Label>
      <Button layoutX="71.0" layoutY="458.0" mnemonicParsing="false" onAction="#close" prefWidth="65.0" text="بستن" AnchorPane.bottomAnchor="15.0">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
